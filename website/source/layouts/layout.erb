<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "RSpec Tutorial" %></title>
    <%= stylesheet_link_tag "normalize" %>
    <%= stylesheet_link_tag "nav_style" %>
    <%= stylesheet_link_tag "header_style" %>
    <%= stylesheet_link_tag "footer_style" %>
    <%= stylesheet_link_tag "RSpectest" %>
    <%= stylesheet_link_tag "all" %>

  </head>

  <body class="<%= page_classes %>">
    <%= partial "header" %>
    <%= partial "navbar" %>
    <%= yield%>
    
  
 
  </body>
</html>
